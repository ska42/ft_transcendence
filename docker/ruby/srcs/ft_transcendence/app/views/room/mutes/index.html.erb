<p id="notice"><%= notice %></p>

<h1>Room Mutes</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>By</th>
      <th>Room</th>
      <th>End at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @room_mutes.each do |room_mute| %>
      <tr>
        <td><%= room_mute.user_id %></td>
        <td><%= room_mute.by_id %></td>
        <td><%= room_mute.room_id %></td>
        <td><%= room_mute.end_at %></td>
        <td><%= link_to 'Show', room_mute %></td>
        <td><%= link_to 'Edit', edit_room_mute_path(room_mute) %></td>
        <td><%= link_to 'Destroy', room_mute, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Room Mute', new_room_mute_path %>
