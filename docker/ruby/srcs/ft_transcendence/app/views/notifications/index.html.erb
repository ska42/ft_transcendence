<p id="notice"><%= notice %></p>

<h1>Notifications</h1>

<table>
	<thead>
		<tr>
			<th>Title</th>
			<th>Date</th>
			<th colspan="3"></th>
		</tr>
	</thead>

	<tbody>
		<% @notifications.each do |notification| %>
			<tr>
				<td><%= notification.title %></td>
				<td><%= notification.updated_at %></td>
				<td><%= link_to 'Open', notification %></td>
			</tr>
		<% end %>
	</tbody>
</table>

<br>
